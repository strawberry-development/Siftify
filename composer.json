{
    "name": "siftifyvendor/siftify",
    "description": "Advanced query filtering for Laravel applications",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Strawberry"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "SiftifyVendor\\Siftify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SiftifyVendor\\Siftify\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SiftifyVendor\\Siftify\\SiftifyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
