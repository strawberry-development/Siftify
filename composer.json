{
    "name": "strawberry-development/siftify",
    "description": "Laravel package for managing HTTP request filtering.",
    "type": "library",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Strawberry"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "strawberry-development\\Siftify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "strawberry-development\\Siftify\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "strawberry-development\\Siftify\\SiftifyServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
